      {"tag": "003", "ind1": "1", "subfields": [{"code": "a", "value": "b"}]},
      {"tag": "003", "ind1": "1", "ind2": "2", "subfields": [{"code": "a", "value": "b"}]},
      {"tag": "003", "ind1": "1", "ind2": "2", "subfields": [{"code": "a", "value": "b"}, {"code": "c", "value": "d"}]}
